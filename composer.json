{
    "name": "eftec/minilang",
    "description": "A mini scripting language for php",
    "type": "library",
    "keywords": [
        "php"
        ,"script"
        ,"scripting"
        ,"language"
    ],
    "homepage": "https://github.com/EFTEC/MiniLang",
    "authors": [
        {
            "name": "Jorge Castro Castillo",
            "homepage": "https://github.com/EFTEC/"
        }
    ],
    "minimum-stability": "beta",
    "license": "LGPL-3.0",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*"
    },
    "require-dev": {
      "phpunit/phpunit": "^8.5.36"
    },
    "autoload": {
      "psr-4": {
        "eftec\\minilang\\": "lib/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "eftec\\tests\\": "tests/"
        }
    }
}
